<!--pages/integral/integral.wxml-->
<view class="container">
  <!--赠送积分内容-->
  <view class="integral" wx:for="{{pointList}}" wx:key="">
    <!--赠送积分-->
    <view class="sign">
      <view class="give">{{item.updRemark}}</view>
      <view class="number">获得{{item.score}}积分</view>
    </view>
    <!--变更时间-->
    <view class="time change">变更时间：{{item.updTime}}</view>
    <!--过期时间-->
    <view class="time past">当前总积分：{{item.thisScore}}</view>
  </view>
</view>