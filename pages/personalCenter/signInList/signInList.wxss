/*头部背景*/

.bg {
  width: 100%;
  height: 381rpx;
}

.header {
 color: #666;
 font-size: 24rpx;
 padding: 30rpx 30rpx 0rpx 30rpx;
}

.header-text {
  position: absolute;
  left: 0;
  right: 0;
  text-align: center;
  top: 200rpx;
  color: #fff;
}


.calendar {
  margin: 30rpx;
  box-shadow: 0rpx 0rpx 10rpx 5rpx #f2f2f2;
  border-radius: 10rpx;
}

.day-selected-round {
  color: #333!important;
  background: #fff!important;
}

