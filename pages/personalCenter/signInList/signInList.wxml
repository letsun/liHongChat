<!--pages/delicious/signIn/signIn.wxml-->

<view class="container">
  <!-- 头部背景 -->
  <view class="header">
    温馨提示：红色字体代表您已经签到过，每日签到可获得10积分
  </view>

  <view class="calendar">
    <calendar class='content' mode='normal' selectedMode="{{selectedMode}}"  selected-date='{{selectedDate}}' begin-date='{{beginDate}}' end-date='{{endDate}}'  bindOnMonthChange="onMonthChange" bindOnDayClick='onDayClick' bindOnRangeComplete='onRangeComplete'
      language='' days-style='{{daysStyle}}' />
  </view>

  



</view>