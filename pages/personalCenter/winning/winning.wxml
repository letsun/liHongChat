<!--pages/winning/winning.wxml-->
<view class="integral">
  <view class="record" wx:for="{{lotteryList}}" wx:key=''>
    <!-- 图片 -->
    <image class="packet" src="{{item.prizePic}}"></image>
    <view class="win">
      <!-- 获得物品 -->
      <view class="get">获得{{item.prizeName}}</view>
      <!-- 获取时间 -->
      <view class="day">{{item.lotteryTime}}</view>
    </view>
    <!-- 活动 -->
    <view class="activity">一物一码活动</view>
  </view>

</view>
