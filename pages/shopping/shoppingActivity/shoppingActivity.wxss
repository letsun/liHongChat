}

.result-win .result-content {
  position: absolute;
  left: 50%;
  top: 46%;
  z-index: 1;
  width: 607rpx;
  height: 690rpx;
  transform: translate(-50%,-50%);
  background: url('https://qdwzvue-1254182596.cos.ap-guangzhou.myqcloud.com/qdwzAct/liHongChat/1_124.png') no-repeat;
  background-size: 100% 100%;
  box-sizing: border-box;
}

.result-title {
  display: block;
  margin: 0 auto;
  margin-top: 170rpx;
}

.result-title1 {
  width: 308rpx;
}

.result-title2 {
  width: 188rpx;
}

.result-dec {
  margin-top: 116rpx;
}

.result-amount {
  color: #f84548;
  font-size: 36rpx;
  font-weight: bold;
  text-align: center;
  padding-top: 50rpx;
}

.result-amount text {
  font-size: 60rpx;
}

.result-tip {
  margin-top: 20rpx;
  color: #676566;
  font-size: 30rpx;
  text-align: center;
  line-height: 30rpx;
}

.btn-wra {
  position: absolute;
  left: 0;
  bottom: 40rpx;
  z-index: 1;
  width: 100%;
  box-sizing: border-box;
  display: flex;
}

.result-btn {
  display: block;
  margin: 0 auto;
  width: 330rpx;
}
.linqu {
  margin: auto;
