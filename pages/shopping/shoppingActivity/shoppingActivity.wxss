/* pages/shopping/shoppingActivity/shoppingActivity.wxss */

page {
  box-sizing: border-box;
}

.bg-image {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  height: 100%;
  width: 100%;
}

.header {
  position: relative;
  width: 100%;
}

.logo-img {
  height: 100rpx;
  width: 140rpx;
  margin: 0rpx 30rpx;
}

.header-text {
  text-align: center;
  margin-bottom: 30rpx;
}

.text-con {
  position: relative;
  display: inline-block;
  margin: 15rpx 0rpx;
  padding: 10rpx 40rpx 15rpx 40rpx;
  box-sizing: border-box;
  border-radius: 40rpx;
  background: #a5d483;
  color: #fff;
}

.header-text span {
  color: #fdfa64;
  font-size: 36rpx;
  font-weight: bold;
  display: inline-block;
}

.header-hd {
  position: absolute;
  right: 0;
  top: 40rpx;
}

.header-hd image {
  height: 113rpx;
  width: 43rpx;
  display: block;
}

.center {
  width: 642rpx;
  height: 791rpx;
  margin: 0 auto;
  box-sizing: border-box;
  position: relative;
}

.con {
  width: 580rpx;
  height: 575rpx;
  margin: 0 auto;
  box-sizing: border-box;
  position: absolute;
  top: 30rpx;
  left: 30rpx;
  z-index: 9;
}

.center-bg {
  width: 642rpx;
  height: 791rpx;
}

.center-bg1 {
  position: absolute;
  left: 0;
  right: 0;
  height: 643rpx;
  width: 643rpx;
  animation: dg2 0.8s ease-out infinite;
}

.center-bg2 {
  position: absolute;
  left: 0;
  right: 0;
  height: 643rpx;
  width: 643rpx;
  animation: dg1 0.8s ease-out infinite;
}

@keyframes dg1{
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}

@keyframes dg2{
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}


.list {
  width: 185rpx;
  height: 185rpx;
  position: absolute;
  /* background-color: #fff; */
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  justify-content: center;
  align-content: center;
  align-items: center;
}



/* 抽奖过程中选定样式 */

.list_all.change_in {
  background: #fff;
  border: 6rpx #fccd0d solid;
  border-radius: 10rpx;
}


.list_all {
  position: absolute;
  left: -1rpx;
  right: -3rpx;
  top: -1rpx;
  bottom: 12rpx;
  border: 6rpx #fff solid;
  border-radius: 20rpx;
}

.maska {
  position: absolute;
  top: -12rpx;
  bottom: -20rpx;
  left: -10rpx;
  right: -10rpx;
  
  background: rgba(0, 0, 0, 0.2);
}

.maskb {
  position: absolute;
  top: 0rpx;
  bottom: 0rpx;
  left: 0rpx;
  right: 0rpx;
  
  background: transparent;
}

.list-item {
  position: absolute;
  top: 20rpx;
  left: 0rpx;
  right: 0rpx;
}

.list_t {
  width: 90%;
  height: 80rpx;
  margin: auto;
}

.list_t image {
  object-fit: cover;
  height: 80rpx;
  max-width: 80rpx;
  margin: auto;
  display: block;
}

.list_img {
  width: 160rpx;
  height: 160rpx;
  border-radius: 20rpx;
  display: block;
  margin: auto;
  
}

.list-bg {
  height: 184rpx;
  width: 188rpx;
  position: absolute;
  top: 0;
  left: 0;
}

.list_b {
  width: 90%;
  margin: auto;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

/* 根据TOP */

.list:nth-child(1) {
  left: 50%;
  top: 50%;
  color: #fff;
  /* background-color: #1cbbb4; */
  display: flex;
  justify-content: center;
  align-content: center;
  align-items: center;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}

.list-text {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  text-align: center;
}

.list-texta {
  position: absolute;
  left: -5rpx;
  right: -7rpx;
  bottom: -1rpx;
  top: -9rpx;
  
  background: rgba(0, 0, 0, 0.2);
}


.list-text1 {
  margin-top: 30rpx;
  font-size: 50rpx;
}

.list-text2 {
  margin-top: 10rpx;
}

.list.grey:nth-child(1) {
  /* background-color: #5f9ea0; */
}

.list.grey:nth-child(1) image {
  height: 185rpx;
  width: 189rpx;
}

.list:nth-child(2), .list:nth-child(3), .list:nth-child(4) {
  top: 0;
}

.list:nth-child(2) .maska{
  bottom: -21rpx;
}
.list:nth-child(4) .maska{
  bottom: -21rpx;
}

.list:nth-child(6), .list:nth-child(7), .list:nth-child(8) {
  bottom: 0;
}

.list:nth-child(5), .list:nth-child(9) {
  top: 50%;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
}

/* 根据LEFT */

.list:nth-child(2), .list:nth-child(8), .list:nth-child(9) {
  left: 0;
}

.list:nth-child(3), .list:nth-child(7) {
  left: 50%;
  -webkit-transform: translateX(-50%);
  transform: translateX(-50%);
}

.list:nth-child(4), .list:nth-child(5), .list:nth-child(6) {
  right: 0;
}



.con-btn {
  position: absolute;
  left: 0;
  right: 0rpx;
  bottom: 0rpx;
  text-align: center;
}

.con-btn image {
  width: 480rpx;
  height: 100rpx;
  display: block;
  margin: auto;
  margin-bottom: -1rpx;
}

.result-win {
  position: absolute;
  left: 0;
  top: 0;
  z-index: 99999;
  display: none;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
}

.result-win .result-bg {
  position: absolute;
  left: 50%;
  top: 41%;
  width: 750rpx;
  height: 735rpx;
  transform: translate(-50%,-50%);
  background: url('https://qdwzvue-1254182596.cos.ap-guangzhou.myqcloud.com/qdwzAct/liHongChat/1_118.png') no-repeat;
  background-size: 100% 100%;
  animation: shansuo 1s ease-out infinite;
}

@keyframes shansuo{
  from {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}

.result-win .result-bg .ani {
  position: absolute;
}

.result-win .result-bg .result-img1 {
  height: 30rpx;
  width: 30rpx;
  top: 100rpx;
  left: 150rpx;
}

.result-win .result-bg .result-img2 {
  height: 30rpx;
  width: 30rpx;
  top: 200rpx;
  right: 140rpx;
}

.result-win .result-bg .result-img3 {
  height: 40rpx;
  width: 40rpx;
  left: 70rpx;
  top: 420rpx;
}

.result-win .result-bg .result-img4 {
  height: 24rpx;
  width: 24rpx;
  top: 490rpx;
  right: 100rpx;
}

.result-win .result-bg .result-img5 {
  height: 24rpx;
  width: 24rpx;
  top: 740rpx;
  left: 100rpx;
}

.result-win .result-bg .result-img6 {
  height: 24rpx;
  width: 24rpx;
  top: 790rpx;
