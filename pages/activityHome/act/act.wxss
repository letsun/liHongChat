.block {
	display: block;
}

.none {
	display: none;
}

.blockAnmiton {
	display: block;
	animation: left 0.9s ease-in forwards;
}

.noneAnmiton {
	
	animation: right 0.9s ease-in forwards;
	display: none;
}


.shake {
	animation: shakeAni 2s linear infinite;
	-webkit-animation: shakeAni 2s linear infinite;
}

@keyframes left {
	0% {
		left: 750rpx;
	}

	100% {
		left: 0;
	}
}


@keyframes right {
	0% {
		left:0 ;
	}

	100% {
		left: 750rpx;
	}
}


@keyframes shakeAni {
	0% {
		transform: rotate(4deg);
	}

	50% {
		transform: rotate(-4deg);
	}

	100% {
		transform: rotate(4deg);
	}
}